       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="compactDisc" class="com.simple.spring4.chapter2.xmlconfig.SgtPeppers"></bean>

    <bean  class="com.simple.spring4.chapter2.xmlconfig.CDPlayer">
